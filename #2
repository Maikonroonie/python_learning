list=["jeden", "dwa", "trzy", 4, 5]
print(list[3])#3 elemetn zostanie wypisany(4 element)
print(list[-1])#ostatmi elenetmt listy
print(list[1:4])#od 1 do 4 ale bez 4
numbers=[2, 4, 6, 8, 10, 12]
# numbers.extend(list) 
print(numbers)
list.append("cwel")
list.insert(1, "krowa")
list.remove("jeden")
# list.clear() czyscin liste
list.pop() #usuwa osattni element listy
print(list.index("dwa"))#printuje dwa jesli jest
print(list.count("krowa"))
print(numbers.sort())# kolejnos alfabetyczna ale w liscie nie moze byc stringow i intow jednoczesnie
list.reverse()
