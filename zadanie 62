def sito(N):
    sito=[True for i in range(N)]
    for i in range(2,N):
        if sito[i]:
            print(i, "jest liczba pierwszÄ…")
            for j in range(2*i, N, i):
                sito[j]=False
    return [i for i in range(2,N) if sito[i]]
    
print(sito(100))






