def zad1(t1,t2):
    n1=len(t1)
    n2=len(t2)
    for i in range(1,24):
        for ix1 in range(n1-i+1):
            for ix2 in range(n2-(24-i)+1):
                sum=sum_tab(t1,ix1,i)+sum_tab(t2,ix2,24-i)
                if(is_pow(sum)):return True
    return False

def sum_tab(tab,ix,l):
    sum=0
    for i in range(ix,ix+l):
        sum+=tab[i]
    return sum

def is_pow(x):
    a=2
    while a<=(x**(1/2)):
        x_copy=x
        while x_copy%a==0: x_copy//=a
        if x_copy==1: 
            return True
        a+=1
    return False
print(is_pow(9))
